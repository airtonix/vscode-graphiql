{
  "root": "packages/nx-vscode",
  "sourceRoot": "packages/nx-vscode/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/nx-vscode",
        "main": "packages/nx-vscode/src/index.ts",
        "tsConfig": "packages/nx-vscode/tsconfig.lib.json",
        "assets": [
          "packages/nx-vscode/*.md",
          {
            "input": "./packages/nx-vscode/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/nx-vscode/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/nx-vscode",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/nx-vscode",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/nx-vscode/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/nx-vscode"
      ],
      "options": {
        "jestConfig": "packages/nx-vscode/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
