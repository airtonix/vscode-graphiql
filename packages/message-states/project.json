{
    "root": "packages/message-states",
    "sourceRoot": "packages/message-states/src",
    "projectType": "library",
    "tags": [
        "scope:shared",
        "type:constants"
    ],
    "generators": {},
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": [
                "{options.outputPath}"
            ],
            "defaultConfiguration": "production",
            "options": {
                "main": "packages/message-states/src/index.ts",
                "outputPath": "dist/libs/message-states",
                "tsConfig": "packages/message-states/tsconfig.lib.json"
            },
            "dependsOn": [
                {
                    "projects": "dependencies"
                }
            ],
            "configurations": {
                "production": {}
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "packages/message-states/src/**/*.{ts,js}"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/packages/message-states"
            ],
            "options": {
                "jestConfig": "packages/message-states/jest.config.js",
                "passWithNoTests": true
            }
        }
    }
}
