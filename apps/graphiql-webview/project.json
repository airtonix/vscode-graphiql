{
  "root": "apps/graphiql-webview",
  "sourceRoot": "apps/graphiql-webview/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/graphiql-webview",
        "command": "pnpx vite build",
        "outputPath": "dist/apps/graphiql-extension"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "pnpx vite dev  --host"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/graphiql-webview/**/*.{ts,js,tsx,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/webview"
      ],
      "options": {
        "jestConfig": "apps/graphiql-webview/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
