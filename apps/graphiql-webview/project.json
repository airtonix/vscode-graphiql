{
    "root": "apps/graphiql-webview",
    "sourceRoot": "apps/graphiql-webview/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/workspace:run-commands",
            "outputs": [
                "{options.outputPath}"
            ],
            "defaultConfiguration": "production",
            "options": {
                "command": "DEBUG=vite-tsconfig-paths  vite build",
                "cwd": "apps/graphiql-webview"
            },
            "configurations": {
                "production": {}
            },
            "dependsOn": [
                {
                    "projects": "dependencies"
                }
            ]
        },
        "serve": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "command": "vite dev",
                "cwd": "apps/graphiql-webview"
            },
            "configurations": {
                "production": {}
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "apps/graphiql-webview/**/*.{ts,js,tsx,jsx}"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/apps/webview"
            ],
            "options": {
                "jestConfig": "apps/graphiql-webview/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
