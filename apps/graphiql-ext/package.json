{
  "name": "graphiqlexplorer",
  "version": "0.0.1",
  "publisher": "airtonix",
  "main": "dist/graphiql.cjs.js",
  "private": true,
  "engines": {
    "vscode": "^1.66.0"
  },
  "activationEvents": [
    "onCommand:vscodegraphqlexplorer.exploreSchema"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscodegraphqlexplorer.exploreSchema",
        "title": "Explore schema..."
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == graphql",
          "command": "vscodegraphqlexplorer.exploreSchema"
        }
      ]
    }
  },
  "vsce": {},
  "scripts": {
    "vscode:prepublish": "yarn build",
    "build": "preconstruct build",
    "package": "vsce package --yarn",
    "list": "vsce ls",
    "login": "vsce login"
  },
  "dependencies": {
    "@vscodegraphqlexplorer/lib-message-states": "0.0.1",
    "@vscodegraphqlexplorer/lib-types": "0.0.1",
    "typescript": "4.5.5"
  },
  "devDependencies": {
    "@preconstruct/cli": "2.1.5",
    "@types/node": "17.0.19",
    "@types/vscode": "1.66.0",
    "ts-node": "10.5.0",
    "vsce-yarn-patch": "1.66.2",
    "graphiqlexplorer": "*"
  },
  "peerDependencies": {
    "vscode": "1.1.37"
  }
}
