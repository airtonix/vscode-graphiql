{
  "name": "vscode-graphiql",
  "version": "0.0.1",
  "publisher": "airtonix",
  "private": true,
  "engines": {
    "vscode": "^1.66.0"
  },
  "main": "extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/airtonix/vscode-graphiql"
  },
  "activationEvents": [
    "onCommand:vscodegraphqlexplorer.exploreSchema"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscodegraphiql.exploreSchema",
        "title": "Explore schema..."
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == graphql",
          "command": "vscodegraphiql.exploreSchema"
        }
      ]
    }
  }
}
