{
  "name": "vscode-graphiql",
  "version": "0.0.1",
  "publisher": "airtonix",
  "author": {
    "email": "airtonix@users.noreply.github.com",
    "name": "Zenobius Jiricek"
  },
  "private": true,
  "engines": {
    "vscode": "^1.66.0"
  },
  "main": "src/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/airtonix/vscode-graphiql"
  },
  "scripts": {
    "license": "pnpx license",
    "watch": "tsc --project ./tsconfig.app.json --watch",
    "compile": "tsc --project ./tsconfig.app.json --outDir ../../dist/apps/graphiql-extension"
  },
  "activationEvents": [
    "onCommand:vscodegraphiql.exploreSchema"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscodegraphiql.exploreSchema",
        "title": "Explore schema..."
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == graphql",
          "command": "vscodegraphiql.exploreSchema"
        }
      ]
    }
  }
}
