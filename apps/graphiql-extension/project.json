{
  "root": "apps/graphiql-extension",
  "sourceRoot": "apps/graphiql-extension/src",
  "projectType": "application",
  "implicitDependencies": [
    "vscode-graphiql-webview"
  ],
  "targets": {
    "estrella": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/graphiql-extension",
        "command": "node ./estrella.build.js --outputPath=../../dist/apps/graphiql-extension",
        "outputPath":[
          "dist/apps/graphiql-extension"
        ]
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "copyWebView": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "cp -R ./dist/apps/graphiql-webview/* ./dist/apps/graphiql-extension/",
        "outputPath":[
          "dist/apps/graphiql-extension"
        ]
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "package": {
      "executor": "./packages/nx-vscode:package",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/graphiql-extension"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        },
        {
          "target": "estrella",
          "projects": "self"
        }
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/graphiql-extension/**/*.{ts,js,tsx,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/extension"
      ],
      "options": {
        "jestConfig": "apps/graphiql-extension/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
